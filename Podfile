# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'RunCoin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RunCoin
    pod 'Firebase/Core'
    pod 'Firebase'
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    pod 'SVProgressHUD'
    pod 'GoogleSignIn'
    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'FacebookShare'
    pod 'FBSDKLoginKit'
    pod 'SwiftyJSON'
    pod 'FirebaseStorage'
end
