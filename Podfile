target 'RunCoin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RunCoin
    pod 'Firebase/Core'
    pod 'Firebase'
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    pod 'SVProgressHUD'
    pod 'SwiftyJSON'
    pod 'FirebaseStorage'
    pod 'SDWebImage'
    pod 'Bolts'
end
